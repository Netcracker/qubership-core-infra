{
  "assignAutomerge": true,
  "assigneesFromCodeOwners": true,
  "autodiscover": false,
  "binarySource": "install",
  "branchConcurrentLimit": 25,
  "branchPrefix": "renovate-main/",
  "enabled": true,
  "globalExtends": [
    ":ignoreModulesAndTests",
    ":dependencyDashboard"
  ],
  "labels": [
    "renovate:core",
    "branch:main"
  ],
  "packageRules": [
    {
      "matchManagers": [
        "maven"
      ],
      "matchPackageNames": [
        "/.*/"
      ],
      "addLabels": [
        "manager:maven"
      ]
    },
    {
      "matchManagers": [
        "dockerfile"
      ],
      "matchPackageNames": [
        "/.*/"
      ],
      "addLabels": [
        "manager:docker"
      ]
    },
    {
      "matchManagers": [
        "gomod"
      ],
      "matchPackageNames": [
        "/.*/"
      ],
      "addLabels": [
        "manager:go"
      ]
    },
    {
      "matchManagers": [
        "github-actions"
      ],
      "matchPackageNames": [
        "/.*/"
      ],
      "addLabels": [
        "manager:github-actions"
      ]
    },
    {
      "matchPackageNames": [
        "/.*/"
      ],
      "allowedVersions": "!/.*(redhat|composite|groovyless|jboss|atlassian|preview|does-not-exist|-SNAPSHOT).*/"
    },
    {
      "matchManagers": [
        "maven"
      ],
      "groupName": "maven",
      "groupSlug": "maven",
      "addLabels": [
        "group:maven"
      ]
    },
    {
      "matchManagers": [
        "gomod"
      ],
      "matchUpdateTypes": [
        "major"
      ],
      "groupName": "go-major",
      "groupSlug": "go-major",
      "addLabels": [
        "group:go"
      ]
    },
    {
      "matchManagers": [
        "gomod"
      ],
      "matchUpdateTypes": [
        "minor"
      ],
      "groupName": "go-minor",
      "groupSlug": "go-minor",
      "addLabels": [
        "group:go"
      ]
    },
    {
      "matchManagers": [
        "gomod"
      ],
      "matchUpdateTypes": [
        "patch"
      ],
      "groupName": "go-patch",
      "groupSlug": "go-patch",
      "addLabels": [
        "group:go"
      ]
    },
    {
      "matchManagers": [
        "gomod"
      ],
      "matchUpdateTypes": [
        "digest"
      ],
      "groupName": "go-digest",
      "groupSlug": "go-digest",
      "addLabels": [
        "group:go"
      ]
    },
    {
      "matchManagers": [
        "gomod"
      ],
      "matchDepNames": [
        "go"
      ],
      "matchDepTypes": [
        "golang"
      ],
      "rangeStrategy": "bump",
      "addLabels": [
        "group:go-version"
      ]
    },
    {
      "matchManagers": [
        "github-actions"
      ],
      "groupName": "github-actions"
    },
    {
      "matchPackageNames": [
        "org.assertj:assertj-core"
